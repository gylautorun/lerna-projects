{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "plugins": [
    "@babel",
    "import",
    "react",
    "react-hooks"
  ],
  "rules": {
    "react-hooks/exhaustive-deps": [
      "error"
    ],
    "react/jsx-boolean-value": [
      "error",
      "never"
    ],
    "react/jsx-closing-bracket-location": [
      "error",
      "line-aligned"
    ],
    "react/jsx-curly-spacing": [
      "error",
      {
        "when": "never",
        "children": true
      }
    ],
    "react/jsx-equals-spacing": [
      "error",
      "never"
    ],
    "react/jsx-filename-extension": [
      "error",
      {
        "allow": "as-needed",
        "extensions": [
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "react/jsx-first-prop-new-line": [
      "error",
      "multiline-multiprop"
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ],
    "react/jsx-indent": [
      "error",
      4
    ],
    "react/jsx-max-props-per-line": [
      "error",
      {
        "maximum": 1,
        "when": "multiline"
      }
    ],
    "react/jsx-pascal-case": [
      "error",
      {
        "allowAllCaps": false
      }
    ],
    "react/jsx-no-bind": [
      "error",
      {
        "ignoreRefs": false,
        "allowArrowFunctions": true,
        "allowBind": false,
        "allowFunctions": false,
        "ignoreDOMComponents": false
      }
    ],
    "react/jsx-tag-spacing": [
      "error",
      {
        "closingSlash": "never",
        "beforeSelfClosing": "always",
        "afterOpening": "never",
        "beforeClosing": "never"
      }
    ],
    "react/jsx-wrap-multilines": [
      "error",
      {
        "declaration": "parens-new-line",
        "assignment": "parens-new-line",
        "return": "parens-new-line",
        "arrow": "parens-new-line",
        "condition": "parens-new-line",
        "logical": "parens-new-line",
        "prop": "parens-new-line"
      }
    ],
    "react/no-array-index-key": [
      "error"
    ],
    "react/no-will-update-set-state": [
      "error",
      "disallow-in-func"
    ],
    "react/no-multi-comp": [
      "warn",
      {
        "ignoreStateless": true
      }
    ],
    "react/prefer-es6-class": [
      "error",
      "always"
    ],
    "react/prefer-stateless-function": [
      "error"
    ],
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": false
      }
    ],
    "react/style-prop-object": [
      "error"
    ],
    "react/void-dom-elements-no-children": [
      "error"
    ],
    "react/forbid-foreign-prop-types": [
      "error"
    ],
    "react/no-danger": [
      "error"
    ],
    "react/jsx-handler-names": [
      "warn",
      {
        "eventHandlerPrefix": false,
        "eventHandlerPropPrefix": "(on|after|before|render)",
        "checkLocalVariables": false,
        "checkInlineFunction": false
      }
    ],
    "react/sort-comp": [
      "off"
    ],
    "react/jsx-no-duplicate-props": [
      "error",
      {
        "ignoreCase": true
      }
    ],
    "react/jsx-closing-tag-location": [
      "error"
    ],
    "react/jsx-props-no-multi-spaces": [
      "error"
    ],
    "react-hooks/rules-of-hooks": [
      "error"
    ],
    "react/display-name": [
      2
    ],
    "react/jsx-key": [
      2
    ],
    "react/jsx-no-comment-textnodes": [
      2
    ],
    "react/jsx-no-target-blank": [
      2
    ],
    "react/jsx-no-undef": [
      2
    ],
    "react/jsx-uses-react": [
      2
    ],
    "react/jsx-uses-vars": [
      2
    ],
    "react/no-children-prop": [
      2
    ],
    "react/no-danger-with-children": [
      2
    ],
    "react/no-deprecated": [
      2
    ],
    "react/no-direct-mutation-state": [
      2
    ],
    "react/no-find-dom-node": [
      2
    ],
    "react/no-is-mounted": [
      2
    ],
    "react/no-render-return-value": [
      2
    ],
    "react/no-string-refs": [
      2
    ],
    "react/no-unescaped-entities": [
      2
    ],
    "react/no-unknown-property": [
      2
    ],
    "react/no-unsafe": [
      0
    ],
    "react/prop-types": [
      2
    ],
    "react/react-in-jsx-scope": [
      2
    ],
    "react/require-render-return": [
      2
    ],
    "import/named": [
      "off"
    ],
    "import/no-absolute-path": [
      "error"
    ],
    "import/no-deprecated": [
      "error"
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true,
        "optionalDependencies": true,
        "peerDependencies": true
      }
    ],
    "import/unambiguous": [
      "error"
    ],
    "import/no-commonjs": [
      "error"
    ],
    "import/no-amd": [
      "error"
    ],
    "import/first": [
      "error"
    ],
    "import/extensions": [
      "error",
      {
        "js": "never",
        "jsx": "never",
        "mjs": "never"
      }
    ],
    "import/newline-after-import": [
      "error"
    ],
    "import/no-named-default": [
      "error"
    ],
    "import/no-dynamic-require": [
      "error"
    ],
    "import/no-webpack-loader-syntax": [
      "warn"
    ],
    "import/no-mutable-exports": [
      "error"
    ],
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ]
      }
    ],
    "import/no-named-as-default-member": [
      "error"
    ],
    "import/no-duplicates": [
      "error"
    ],
    "import/no-unresolved": [
      "error"
    ],
    "import/namespace": [
      "error"
    ],
    "import/default": [
      "error"
    ],
    "import/export": [
      "error"
    ],
    "import/no-named-as-default": [
      "warn"
    ],
    "@babel/new-cap": [
      "error",
      {
        "newIsCap": true,
        "capIsNew": true,
        "properties": true
      }
    ],
    "object-curly-spacing": [
      "error",
      "never"
    ],
    "@babel/object-curly-spacing": [
      "error",
      "never"
    ],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1,
        "flatTernaryExpressions": false,
        "offsetTernaryExpressions": false,
        "ignoreComments": false
      }
    ],
    "generator-star-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "array-bracket-spacing": [
      "error",
      "never"
    ],
    "curly": [
      "error",
      "all"
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "no-new-wrappers": [
      "error"
    ],
    "no-eval": [
      "error"
    ],
    "no-self-compare": [
      "error"
    ],
    "radix": [
      "error",
      "always"
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "no-multi-spaces": [
      "error",
      {
        "ignoreEOLComments": true,
        "exceptions": {
          "Property": true,
          "BinaryExpression": true,
          "VariableDeclarator": true,
          "ImportDeclaration": true
        }
      }
    ],
    "no-unneeded-ternary": [
      "error",
      {
        "defaultAssignment": false
      }
    ],
    "no-new-object": [
      "error"
    ],
    "no-spaced-func": [
      "error"
    ],
    "no-trailing-spaces": [
      "error"
    ],
    "one-var": [
      "error",
      "never"
    ],
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": false,
        "allowTemplateLiterals": false
      }
    ],
    "semi": [
      "error",
      "always"
    ],
    "semi-spacing": [
      "error"
    ],
    "keyword-spacing": [
      "error"
    ],
    "key-spacing": [
      "error",
      {
        "beforeColon": false,
        "afterColon": true,
        "mode": "strict"
      }
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "space-before-blocks": [
      "error",
      "always"
    ],
    "computed-property-spacing": [
      "error",
      "never"
    ],
    "space-in-parens": [
      "error",
      "never"
    ],
    "spaced-comment": [
      "error",
      "always",
      {
        "block": {
          "balanced": true
        }
      }
    ],
    "max-len": [
      "error",
      120,
      4,
      {
        "ignoreUrls": true,
        "ignoreComments": false
      }
    ],
    "space-infix-ops": [
      "error"
    ],
    "dot-notation": [
      "error",
      {
        "allowKeywords": true,
        "allowPattern": ""
      }
    ],
    "arrow-spacing": [
      "error"
    ],
    "no-confusing-arrow": [
      "error",
      {
        "allowParens": true
      }
    ],
    "prefer-rest-params": [
      "error"
    ],
    "max-statements-per-line": [
      "error",
      {
        "max": 1
      }
    ],
    "guard-for-in": [
      "error"
    ],
    "no-labels": [
      "error",
      {
        "allowLoop": true,
        "allowSwitch": false
      }
    ],
    "no-extend-native": [
      "error"
    ],
    "no-extra-bind": [
      "error"
    ],
    "no-implied-eval": [
      "error"
    ],
    "no-iterator": [
      "error"
    ],
    "no-lone-blocks": [
      "error"
    ],
    "no-loop-func": [
      "error"
    ],
    "no-octal-escape": [
      "error"
    ],
    "no-proto": [
      "error"
    ],
    "no-label-var": [
      "error"
    ],
    "new-parens": [
      "error",
      "always"
    ],
    "no-array-constructor": [
      "error"
    ],
    "space-unary-ops": [
      "error",
      {
        "words": true,
        "nonwords": false
      }
    ],
    "max-nested-callbacks": [
      "error",
      3
    ],
    "max-depth": [
      "error",
      4
    ],
    "max-params": [
      "warn",
      4
    ],
    "no-var": [
      "error"
    ],
    "no-duplicate-imports": [
      "error"
    ],
    "no-useless-constructor": [
      "error"
    ],
    "no-use-before-define": [
      "error"
    ],
    "no-restricted-globals": [
      "error",
      {
        "name": "name",
        "message": "`name` is not defined, or use `window.name` instead."
      },
      {
        "name": "event",
        "message": "Use local parameter instead."
      },
      {
        "name": "fdescribe",
        "message": "Do not commit `fdescribe`. Use `describe` instead."
      }
    ],
    "no-console": [
      "warn",
      {
        "allow": [
          "warn",
          "error",
          "info"
        ]
      }
    ],
    "arrow-parens": [
      "off"
    ],
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ],
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }
    ],
    "consistent-return": [
      "error",
      {
        "treatUndefinedAsUnspecified": false
      }
    ],
    "unicode-bom": [
      "warn",
      "never"
    ],
    "no-native-reassign": [
      "off"
    ],
    "func-call-spacing": [
      "error",
      "never"
    ],
    "no-dupe-class-members": [
      "error"
    ],
    "no-unreachable-loop": [
      "error"
    ],
    "constructor-super": [
      "error"
    ],
