{
    "name": "@repo/react-mobx-ts-webpack",
    "version": "1.0.0",
    "private": true,
    "main": "index.js",
    "description": "react-mobx-ts-webpack 项目",
    "author": "amoursun",
    "scripts": {
        "lint": "tsc",
        "setup": "node ./scripts/setup",
        "build": "node ./scripts/build",
        "start": "npm run dev",
        "dev": "node ./scripts/dev",
        "dll": "npm run dll:dev && npm run dll:prod",
        "dll:dev": "node scripts/dll --mode=development",
        "dll:prod": "node scripts/dll --mode=production",
        "watch": "webpack --watch --config script/webpack.dev.js",
        "lint:jsx": "eslint --ext .tsx src && eslint --ext .ts src",
        "lint:css": "stylelint --aei .less .css src",
        "precommit": "lint-staged",
        "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
    },
    "dependencies": {
        "@ant-design/icons": "^4.6.4",
        "antd": "^4.24.2",
        "axios": "^0.21.4",
        "dayjs": "^1.10.4",
        "mobx": "^6.1.7",
        "mobx-react": "^7.1.0",
        "mobx-react-router": "^4.1.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router-dom": "^6.4.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.17",
        "@babel/core": "^7.12.17",
        "@babel/plugin-proposal-class-properties": "^7.12.13",
        "@babel/plugin-proposal-decorators": "^7.12.13",
        "@babel/plugin-proposal-json-strings": "^7.12.13",
        "@babel/plugin-proposal-optional-chaining": "^7.12.17",
        "@babel/plugin-syntax-dynamic-import": "^7.8.0",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.12.17",
        "@babel/preset-env": "^7.12.17",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.12.17",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^3.10.1",
        "add-asset-html-webpack-plugin": "^5.0.2",
        "antd-dayjs-webpack-plugin": "^1.0.6",
        "autoprefixer": "^9.8.8",
        "babel-loader": "^8.2.2",
        "babel-plugin-import": "^1.13.3",
        "babel-plugin-react-css-modules": "^5.2.6",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "classnames": "^2.2.6",
        "clean-webpack-plugin": "^3.0.0",
        "commander": "^5.1.0",
        "compression-webpack-plugin": "^7.1.2",
        "core-js": "^3.26.1",
        "copy-webpack-plugin": "^7.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.2",
        "css-minimizer-webpack-plugin": "^1.2.0",
        "cssnano": "^4.1.11",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-webpack-plugin": "^2.5.2",
        "file-loader": "^6.2.0",
        "fs-extra": "^10.1.0",
        "history": "^4.7.2",
        "html-webpack-plugin": "^5.2.0",
        "less": "^4.1.3",
        "less-loader": "^11.1.0",
        "lint-staged": "^10.5.4",
        "mini-css-extract-plugin": "^1.3.8",
        "mockjs": "^1.1.0",
        "os-browserify": "^0.3.0",
        "pre-commit": "^1.2.2",
        "prettier": "^2.2.1",
        "progress-bar-webpack-plugin": "^2.1.0",
        "url-loader": "^4.1.1",
        "raw-loader": "^4.0.2",
        "react-intl-universal": "^2.4.3",
        "react-scripts": "^4.0.2",
        "resolve-url-loader": "^5.0.0",
        "shelljs": "^0.8.5",
        "socket.io-client": "^3.1.1",
        "style-loader": "^2.0.0",
        "stylelint": "^13.10.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-standard": "^20.0.0",
        "terser-webpack-plugin": "^5.3.6",
        "trtc-js-sdk": "^4.8.5",
        "turbo": "^1.6.1",
        "typescript": "^4.4.3",
        "webpack": "^5.69.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    },
    "pre-commit": [
        "precommit",
        "precommit-msg"
    ],
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{css,less}": [
            "stylelint --fix",
            "prettier --write",
            "git add"
        ]
    }
}
